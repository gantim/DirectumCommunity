@model List<DirectumCommunity.Data.Models.Worker>

<h2>Список сотрудников</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Имя</th>
            <th>Город</th>
            <th>День рождение</th>
            <!-- Добавьте другие колонки по вашему усмотрению -->
        </tr>
    </thead>
    <tbody>
        @foreach (var worker in Model)
        {
            <tr>
                <td>@worker.id</td>
                <td>@worker.name</td>
                <td>@worker.city</td>
                <td>@worker.birthday</td>
                <!-- Добавьте другие ячейки данных по вашему усмотрению -->
            </tr>
        }
    </tbody>
</table>
